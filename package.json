{
  "name": "subgraphs-workspace",
  "private": true,
  "workspaces": [
    "subgraphs/*"
  ],
  "scripts": {
    "rtb:codegen": "cd subgraphs/rtb && npm run codegen",
    "rtb:build": "cd subgraphs/rtb && npm run build",
    "rtb:deploy": "cd subgraphs/rtb && npm run deploy",
    "rtb:test": "cd subgraphs/rtb && npm run test",
    "gluex:codegen": "cd subgraphs/gluex && npm run codegen",
    "gluex:build": "cd subgraphs/gluex && npm run build",
    "gluex:deploy": "cd subgraphs/gluex && npm run deploy",
    "gluex:test": "cd subgraphs/gluex && npm run test",
    "codegen": "npm run codegen --workspaces --if-present",
    "build": "npm run build --workspaces --if-present",
    "test": "npm run test --workspaces --if-present"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.51.0",
    "@graphprotocol/graph-ts": "0.31.0"
  },
  "devDependencies": {
    "mustache": "^4.0.1"
  }
}
